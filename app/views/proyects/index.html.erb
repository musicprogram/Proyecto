<p id="notice"><%= notice %></p>

<h1>Listing Proyects</h1>


<div class="media">
  <div class="media-body">
    
    <table class="table table-hover">

  <thead>
    <tr>
      <th>Nombre</th>
      <th>Fecha entrega</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proyects.each do |proyect| %>
      <tr>
        <td><%= proyect.nombre %></td>
        <td><%= proyect.fecha_entrega %></td>
        <td><%= link_to 'Show', proyect %></td>
        <td><%= link_to 'Edit', edit_proyect_path(proyect) %></td>
        <td><%= link_to 'Destroy', proyect, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table></div></div>

<br>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_proyect_path,
            :class => 'btn btn-inverse' %>

